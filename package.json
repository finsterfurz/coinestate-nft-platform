{
  "name": "coinestate-nft-platform",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.5.0",
    "web-vitals": "^2.1.4",
    "ethers": "^6.8.0",
    "joi": "^17.11.0",
    "react-helmet-async": "^1.3.0",
    "axios": "^1.6.0",
    "crypto-js": "^4.2.0",
    "prop-types": "^15.8.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .js,.jsx",
    "lint:fix": "eslint src/ --ext .js,.jsx --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:ci": "npm test -- --coverage --watchAll=false --ci",
    "test:watch": "npm test -- --watch",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:accessibility": "npm test -- --testNamePattern=\"accessibility\"",
    "test:integration": "npm test -- --testNamePattern=\"integration\"",
    "test:unit": "npm test -- --testNamePattern=\"unit|Unit\"",
    "security:audit": "npm audit --audit-level moderate",
    "prebuild": "npm run lint && npm run test:ci",
    "prepare": "husky install",
    "analyze:bundle": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test start http://localhost:3000 cypress:run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": ["warn", { "allow": ["warn", "error"] }],
      "no-unused-vars": "error",
      "react-hooks/exhaustive-deps": "warn",
      "no-eval": "error",
      "no-implied-eval": "error",
      "no-new-func": "error",
      "no-script-url": "error",
      "react/prop-types": "warn",
      "jsx-a11y/anchor-is-valid": "warn",
      "jsx-a11y/no-redundant-roles": "warn",
      "jsx-a11y/aria-role": "error",
      "jsx-a11y/alt-text": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "jest-axe": "^8.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "cypress": "^13.6.0",
    "start-server-and-test": "^2.0.3",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/**/*.stories.{js,jsx}",
      "!src/data/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  }
}